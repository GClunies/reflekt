{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Started IAP/1-0.json",
    "description": "Client side event fired when a user clicks a CTA to subscribe and the \"Go Premium\" screen (or similar variant) pops up.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Started IAP",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events)."
        },
        "currency": {
            "description": "The currency."
        },
        "id": {
            "description": "The product ID."
        },
        "interval": {
            "description": "The subscription billing interval."
        },
        "intervalCount": {
            "description": "The count of billing intervals that have occured for the subscription."
        },
        "paymentPlatform": {
            "description": "The payment platform."
        },
        "price": {
            "description": "The price."
        },
        "productName": {
            "description": "The product name."
        },
        "quantity": {
            "description": "The quantity."
        },
        "sku": {
            "description": "The product SKU."
        }
    },
    "required": [
        "category",
        "currency",
        "id",
        "interval",
        "intervalCount",
        "paymentPlatform",
        "price",
        "productName",
        "quantity",
        "sku"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}