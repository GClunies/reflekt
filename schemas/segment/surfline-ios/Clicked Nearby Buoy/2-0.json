{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Nearby Buoy/2-0.json",
    "description": "Client side event fired when a user clicks View Buoy Data link in the Nearby Buoys carousel.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Nearby Buoy",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "buoyId": {
            "description": "The assigned ID for the buoy.",
            "type": "string"
        },
        "buoyName": {
            "description": "The name of the buoy.",
            "type": "string"
        },
        "category": {
            "description": "The product category this event belongs to (e.g. `kbyg`, `ecommerce`, `sessions`, `travel`, `content`).",
            "enum": [
                "kbyg",
                "ecommerce",
                "sessions",
                "travel",
                "content"
            ],
            "type": "string"
        },
        "location": {
            "description": "Where the event fired on the screen.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The region name.",
            "type": "string"
        }
    },
    "required": [
        "buoyId",
        "buoyName",
        "category",
        "location"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}