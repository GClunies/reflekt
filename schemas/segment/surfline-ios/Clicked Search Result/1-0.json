{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Search Result/1-0.json",
    "description": "Client side event fired when a user selects an item in the search feature.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Search Result",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "location": {
            "description": "Where the event fired on our application.",
            "type": "string"
        },
        "queryTerm": {
            "description": "The text in the search bar.",
            "type": [
                "string",
                "null"
            ]
        },
        "resultName": {
            "description": "The name of the item that the user selected.",
            "type": "string"
        },
        "resultType": {
            "description": "The type of result that the user selected.",
            "type": "string"
        }
    },
    "required": [
        "location",
        "queryTerm",
        "resultName",
        "resultType"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}