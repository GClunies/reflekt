{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Alert/2-0.json",
    "description": "Client side event fired when a user clicks a Surf Alert.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Alert",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "alertState": {
            "description": "The alert state.",
            "enum": [
                "on",
                "off"
            ],
            "type": "string"
        },
        "alertType": {
            "description": "The alert type.",
            "type": "string"
        },
        "alertcategory": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": [
                "string",
                "null"
            ]
        },
        "spotName": {
            "description": "The spot name.",
            "type": [
                "string",
                "null"
            ]
        }
    },
    "required": [
        "alertState",
        "alertType",
        "screenName",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}