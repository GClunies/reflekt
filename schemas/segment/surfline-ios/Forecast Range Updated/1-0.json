{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Forecast Range Updated/1-0.json",
    "description": "Fired when a user updates the date range on any KBYG forecast.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Forecast Range Updated",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "action": {
            "description": "The action taken by the user to update the forecast range (e.g. click, swipe, etc.)",
            "enum": [
                "click",
                "swipe",
                "button"
            ],
            "type": "string"
        },
        "forecastEnd": {
            "description": "When the forecast being viewed ends relative to today (4 = today + 3 days)",
            "type": "integer"
        },
        "forecastStart": {
            "description": "When the forecast being viewed starts relative to today (1 = today)",
            "type": "integer"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The subregion name.",
            "type": "string"
        }
    },
    "required": [],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}