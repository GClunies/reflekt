{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Toggled Cam View/2-0.json",
    "description": "Client side event fired when a user toggles a camera view.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Toggled Cam View",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "camChange": {
            "description": "The type of camera change.",
            "enum": [
                "spot",
                "angle"
            ],
            "type": "string"
        },
        "camId": {
            "description": "The camera ID.",
            "type": "string"
        },
        "camName": {
            "description": "The camera name.",
            "type": "string"
        },
        "playerMode": {
            "description": "The player mode.",
            "type": "string"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The region name.",
            "type": "string"
        }
    },
    "required": [
        "camChange",
        "camId",
        "camName",
        "playerMode",
        "screenName",
        "spotId",
        "spotName",
        "subregionId"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}