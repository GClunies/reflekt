{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Switched Session Wave Views/2-0.json",
    "description": "Client side event fired when a user switches the wave view from the List view to the Map view (and vice versa).",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Switched Session Wave Views",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "daysAgo": {
            "description": "The number of days ago the session was recorded.",
            "type": "integer"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "sessionLatLon": {
            "description": "The session latitude and longitude.",
            "type": "string"
        },
        "sessionRating": {
            "description": "The session rating.",
            "type": "integer"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        }
    },
    "required": [
        "daysAgo",
        "screenName",
        "sessionRating",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}