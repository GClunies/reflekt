{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Fetched Report/1-0.json",
    "description": "Client side event fired when a user views a Spot screen.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Fetched Report",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "hasCam": {
            "description": "Whether the spot has a camera.",
            "type": "boolean"
        },
        "premiumCam": {
            "description": "Whether the spot has a premium camera.",
            "type": "boolean"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "status": {
            "description": "Set to 'error' if there is an error fetching the report.",
            "type": "string"
        }
    },
    "required": [],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}