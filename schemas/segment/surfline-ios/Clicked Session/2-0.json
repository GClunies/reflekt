{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Session/2-0.json",
    "description": "Client side event fired when a user clicks on a Surfline Session in app.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Session",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "daysAgo": {
            "description": "The number of days ago the session was.",
            "type": "integer"
        },
        "hasClip": {
            "description": "Does the session have a clip?",
            "type": "boolean"
        },
        "rating": {
            "description": "The rating of the session.",
            "type": "integer"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "spotReliveRating": {
            "description": "The relive rating of the spot.",
            "type": [
                "integer",
                "null"
            ]
        }
    },
    "required": [
        "daysAgo",
        "hasClip",
        "rating",
        "screenName",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}