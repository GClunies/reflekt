{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Saved Clip/2-0.json",
    "description": "Client side event fired when a user downloads a Cam Rewind.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Saved Clip",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "camId": {
            "description": "The cam ID.",
            "type": "string"
        },
        "camName": {
            "description": "The cam name.",
            "type": "string"
        },
        "daysAgo": {
            "description": "The number of days since the clip was created.",
            "type": "integer"
        },
        "premiumCam": {
            "description": "Whether the clip is a premium cam.",
            "type": "boolean"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        }
    },
    "required": [
        "daysAgo",
        "screenName",
        "spotId",
        "spotName",
        "subregionId"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}