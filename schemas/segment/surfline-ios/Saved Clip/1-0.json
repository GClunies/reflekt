            "description": "The cam ID."
        },
        "camName": {
            "description": "The cam name."
        },
        "daysAgo": {
            "description": "The number of days since the clip was created."
        },
        "premiumCam": {
            "description": "Whether the clip is a premium cam."
        },
        "screenName": {
            "description": "The screen name."
        },
        "spotId": {
            "description": "The spot ID."
        },
        "spotName": {
            "description": "The spot name."
        },
        "subregionId": {
            "description": "The subregion ID."
        }
    },
    "required": [
        "daysAgo",
        "screenName",
        "spotId",
        "spotName",
        "subregionId"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
