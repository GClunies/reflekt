{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Saved Clip/1-0.json",
    "description": "Client side event fired when a user downloads a Cam Rewind.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Saved Clip",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
