{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Manual Rewind/2-0.json",
    "description": "Client side event fired when a user clicks any of the days that brings them into the Cam Rewind flow.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Manual Rewind",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "camId": {
            "description": "The camera ID.",
            "type": "string"
        },
        "camName": {
            "description": "The camera name.",
            "type": "string"
        },
        "daysAgo": {
            "description": "The number of days ago the rewind is from.",
            "type": "integer"
        },
        "premiumCam": {
            "description": "Is camera premium?",
            "type": "boolean"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        }
    },
    "required": [
        "daysAgo",
        "premiumCam",
        "screenName",
        "spotId",
        "spotName",
        "subregionId"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}