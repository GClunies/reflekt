{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Spot Card/1-0.json",
    "description": "Client side event fired when a user clicks a Spot card from the map.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Spot Card",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "hasCam": {
            "description": "Whether or not the spot has a camera."
        },
        "screenName": {
            "description": "The screen name where the user clicked the spot card."
        },
        "spotId": {
            "description": "The spotID."
        },
        "spotName": {
            "description": "The spot name."
        },
        "zoomLevel": {
            "description": "The zoom level of the map."
        }
    },
    "required": [
        "hasCam",
        "screenName",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}