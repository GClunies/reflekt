{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Completed Order/2-0.json",
    "description": "Client side event fired when a user completes the final step in the subscription funnel (started free trial or direct subscription). Can be used to compare what happens on client versus our servers.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Completed Order",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "currency": {
            "description": "The currency.",
            "pattern": "[A-Z]",
            "type": "string"
        },
        "id": {
            "description": "The product ID.",
            "type": "string"
        },
        "interval": {
            "description": "The subscription billing interval.",
            "enum": [
                "month",
                "year"
            ],
            "type": "string"
        },
        "intervalCount": {
            "description": "The count of billing intervals that have occured for the subscription.",
            "type": "integer"
        },
        "orderId": {
            "description": "The order ID.",
            "type": "string"
        },
        "paymentPlatform": {
            "description": "The payment platform.",
            "type": "string"
        },
        "price": {
            "description": "The price.",
            "type": "number"
        },
        "productName": {
            "description": "The product name.",
            "type": "string"
        },
        "products": {
            "description": "The products in the transaction.",
            "type": "string"
        },
        "quantity": {
            "description": "The quantity.",
            "type": "integer"
        },
        "revenue": {
            "description": "The transaction revenue.",
            "type": "number"
        },
        "sku": {
            "description": "The product SKU.",
            "type": "string"
        },
        "total": {
            "description": "The transaction total.",
            "type": "number"
        }
    },
    "required": [
        "currency",
        "id",
        "interval",
        "intervalCount",
        "orderId",
        "paymentPlatform",
        "price",
        "productName",
        "quantity",
        "sku"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}