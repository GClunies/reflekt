{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Edited Session/2-0.json",
    "description": "Client side event fired when a user changes a spot for their Surfline Session.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Edited Session",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "daysAgo": {
            "description": "The number of days since the session was created.",
            "type": "integer"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "sessionEdit": {
            "description": "The type of edit being made to the session.",
            "enum": [
                "Spot",
                "deleted",
                "Name"
            ],
            "type": "string"
        },
        "sessionLatLon": {
            "description": "The session latitude and longitude.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        }
    },
    "required": [
        "daysAgo",
        "screenName",
        "sessionEdit",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}