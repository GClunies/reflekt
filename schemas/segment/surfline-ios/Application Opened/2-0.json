{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Application Opened/2-0.json",
    "description": "Client side event fired when a user opens the Surfline app.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Application Opened",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "build": {
            "description": "The application build.",
            "type": "string"
        },
        "from_background": {
            "description": "Whether the application was opened from the background.",
            "type": "boolean"
        },
        "referring_application": {
            "description": "The application that referred the user to the Surfline.",
            "type": [
                "string",
                "null"
            ]
        },
        "url": {
            "description": "The deep link URL that brought the user to the Surfline app.",
            "type": [
                "string",
                "null"
            ]
        },
        "version": {
            "description": "The application version.",
            "type": "string"
        }
    },
    "required": [
        "build",
        "version"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}