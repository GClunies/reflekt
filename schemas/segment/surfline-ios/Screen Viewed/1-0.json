{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Screen Viewed/1-0.json",
    "description": "Client side event fired when a user views any screen in the app.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Screen Viewed",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "alertsOnCount": {
            "description": "Number of alerts turned on.",
            "type": "integer"
        },
        "alertsSetupCount": {
            "description": "Number of alerts setup.",
            "type": "integer"
        },
        "articleType": {
            "description": "The article type.",
            "type": "string"
        },
        "camId": {
            "description": "The cam ID.",
            "type": "string"
        },
        "category": {
            "description": "The screen category (used to group to related events).",
            "type": "string"
        },
        "centerlat": {
            "description": "The latitude of the center of the map.",
            "type": "number"
        },
        "centerlon": {
            "description": "The longitude of the center of the map.",
            "type": "number"
        },
        "contentType": {
            "description": "The content type.",
            "type": "string"
        },
        "daysAgo": {
            "description": "The number of days ago between a Surfline Session (or wave) and the screen timestamp.",
            "type": "integer"
        },
        "feedType": {
            "description": "The feed type.",
            "type": "string"
        },
        "forecastName": {
            "description": "The forecast name.",
            "type": "string"
        },
        "group": {
            "description": "The group of the screen.",
            "type": "string"
        },
        "hasCam": {
            "description": "Whether the screen has a cam.",
            "type": "boolean"
        },
        "navigationName": {
            "description": "The navigation name.",
            "type": "string"
        },
        "orientation": {
            "description": "The orientation of the screen.",
            "type": "string"
        },
        "premiumCam": {
            "description": "Whether the cam is a premium cam.",
            "type": "boolean"
        },
        "reason": {
            "description": "The reason the event fired.",
            "type": "string"
        },
        "rewindType": {
            "description": "The rewind type.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "spotType": {
            "description": "The spot type.",
            "type": "string"
        },
        "spotsEditedCount": {
            "description": "Number of spots edited.",
            "type": "integer"
        },
        "state": {
            "description": "The state of the screen.",
            "type": "string"
        },
        "status": {
            "description": "The screen view status.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "tabName": {
            "description": "The name of the tab selected on the screen.",
            "type": "string"
        },
        "title": {
            "description": "The title of the screen.",
            "type": "string"
        },
        "zoomLevel": {
            "description": "The zoom level of the map.",
            "type": "number"
        }
    },
    "required": [],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}