{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Cluster/1-0.json",
    "description": "Client side event fired when a user clicks clicks a cluster (if exists) on a map screen.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Cluster",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "centerlat": {
            "description": "The latitude of the center of the map.",
            "type": "number"
        },
        "centerlon": {
            "description": "The longitude of the center of the map.",
            "type": "number"
        },
        "numberOfSpots": {
            "description": "The number of spots in the cluster.",
            "type": "number"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "zoomLevel": {
            "description": "The zoom level of the map.",
            "type": "number"
        }
    },
    "required": [],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}