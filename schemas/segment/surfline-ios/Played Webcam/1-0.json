{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Played Webcam/1-0.json",
    "description": "Client side event fired when a user clicks play on a Spot camera.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Played Webcam",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "fullScreenEnabled": {
            "description": "Whether the camera is full screen."
        },
        "id": {
            "description": "The camera ID."
        },
        "resume": {
            "description": "Whether the camera is being resumed."
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        }
    },
    "required": [
        "fullScreenEnabled",
        "id",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}