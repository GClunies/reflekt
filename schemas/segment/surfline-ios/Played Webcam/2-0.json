{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Played Webcam/2-0.json",
    "description": "Client side event fired when a user clicks play on a Spot camera.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Played Webcam",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "carouselPositionNumber": {
            "description": "Number of the carousel card in order from left to right (first card=0).",
            "type": "integer"
        },
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "fullScreenEnabled": {
            "description": "Whether the camera is full screen.",
            "type": "boolean"
        },
        "id": {
            "description": "The camera ID.",
            "type": "string"
        },
        "resume": {
            "description": "Whether the camera is being resumed.",
            "type": [
                "boolean",
                "null"
            ]
        },
        "screenName": {
            "description": "Name of the screen where the surf camera is played.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        }
    },
    "required": [
        "fullScreenEnabled",
        "id",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}