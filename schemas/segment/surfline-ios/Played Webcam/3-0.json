{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Played Webcam/3-0.json",
    "description": "Client side event fired when a user clicks play on a Spot camera.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Played Webcam",
        "format": "jsonschema",
        "version": "3-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "camId": {
            "description": "The camera ID.",
            "type": "string"
        },
        "camName": {
            "description": "The camera name.",
            "type": "string"
        },
        "carouselPositionNumber": {
            "description": "Number of the carousel card in order from left to right (first card=0).",
            "type": "integer"
        },
        "category": {
            "description": "The product category this event belongs to (e.g. `kbyg`, `ecommerce`, `sessions`, `travel`, `content`).",
            "enum": [
                "kbyg",
                "ecommerce",
                "sessions",
                "travel",
                "content"
            ],
            "type": "string"
        },
        "favorite": {
            "description": "Is the camera favorited by the user?",
            "type": "boolean"
        },
        "fullScreenEnabled": {
            "description": "Whether the camera is full screen.",
            "type": "boolean"
        },
        "numberOfCams": {
            "description": "The number of cameras at the spot.",
            "type": "integer"
        },
        "premiumCam": {
            "description": "Is camera premium?",
            "type": "boolean"
        },
        "screenName": {
            "description": "Name of the screen where the surf camera is played.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The region name.",
            "type": "string"
        },
        "videoType": {
            "description": "The type of video.",
            "enum": [
                "live",
                "highlights"
            ],
            "type": "string"
        }
    },
    "required": [
        "camId",
        "camName",
        "fullScreenEnabled",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}