{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Share Clip/2-0.json",
    "description": "Client side event fired when a user clicks to share a Surfline Sessions clip.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Share Clip",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "daysAgo": {
            "description": "The number of days between when the event fired and the clip timestamp.",
            "type": "integer"
        },
        "eventType": {
            "description": "The event type.",
            "type": "string"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "sessionId": {
            "description": "The session ID.",
            "type": "string"
        },
        "sessionLatLon": {
            "description": "The session latitude and longitude.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "spotReliveRating": {
            "description": "The spot relive rating.",
            "type": "integer"
        }
    },
    "required": [
        "category",
        "daysAgo",
        "screenName",
        "sessionId",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}