{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Attempted Session Sync/1-0.json",
    "description": "Client side event (iWatch) fired when a user attempts to sync a Surfline Session.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Attempted Session Sync",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "batteryLevel": {
            "description": "The battery level.",
            "type": "number"
        },
        "failureReason": {
            "description": "The failure reason (if any).",
            "type": "string"
        },
        "lengthOfSession": {
            "description": "The length of the session in seconds.",
            "type": "number"
        },
        "numberPositions": {
            "description": "The number of positions in the session.",
            "type": "number"
        },
        "sessionEndTime": {
            "description": "The session start time in seconds since epoch.",
            "type": "number"
        },
        "sessionStartTime": {
            "description": "The session start time in seconds since epoch.",
            "type": "number"
        },
        "syncAttempt": {
            "description": "The sync attempt.",
            "type": "integer"
        },
        "syncComplete": {
            "description": "Whether the sync was successful.",
            "type": "boolean"
        },
        "watchOs": {
            "description": "The watch operating system.",
            "type": "string"
        },
        "watchSeries": {
            "description": "The watch series.",
            "type": "string"
        }
    },
    "required": [
        "batteryLevel",
        "numberPositions",
        "syncAttempt",
        "syncComplete",
        "watchOs",
        "watchSeries"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}