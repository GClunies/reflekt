{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Clicked Forecast Range/1-0.json",
    "description": "Web event fired when a user clicks to change the forecast date range on a spot report page.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Forecast Range",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "forecastEnd": {
            "description": "When the forecast being viewed ends relative to today (4 = today + 3 days)",
            "type": "integer"
        },
        "forecastLength": {
            "description": "How many days are in forecast being viewed (4 = 4-day forecast is being viewed)",
            "type": "integer"
        },
        "forecastPeriod": {
            "description": "The forecast period (single day).",
            "type": "integer"
        },
        "forecastStart": {
            "description": "When the forecast being viewed starts relative to today (1 = today)",
            "type": "integer"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The subregion name.",
            "type": "string"
        }
    },
    "required": [],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}