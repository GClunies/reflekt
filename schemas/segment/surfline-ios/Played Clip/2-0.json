{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Played Clip/2-0.json",
    "description": "Client side event fired when a user clicks play on a rewind clip.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Played Clip",
        "format": "jsonschema",
        "version": "2-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "daysAgo": {
            "description": "The number of days since the clip was created.",
            "type": "integer"
        },
        "premiumCam": {
            "description": "Is camera premium?",
            "type": "boolean"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "sessionId": {
            "description": "If the clip played is from a Session, then this value is the sessionId associated with the clip.",
            "type": [
                "string",
                "null"
            ]
        },
        "sessionLatLon": {
            "description": "The session latitude and longitude.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "spotReliveRating": {
            "description": "The spot relive rating.",
            "type": [
                "integer",
                "null"
            ]
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        }
    },
    "required": [
        "daysAgo",
        "screenName",
        "spotId",
        "spotName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}