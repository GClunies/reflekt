{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-ios/Forecast Swiped/1-0.json",
    "description": "Client side event fired when a swipes a forecast (i.e. the user swipes the screen on teh forecast tab to change the day)",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Forecast Swiped",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The product category this event belongs to (e.g. `kbyg`, `ecommerce`, `sessions`, `travel`, `content`).",
            "enum": [
                "kbyg",
                "ecommerce",
                "sessions",
                "travel",
                "content"
            ],
            "type": "string"
        },
        "forecastPeriod": {
            "description": "The forecast period (single day).",
            "type": "integer"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The region name.",
            "type": "string"
        }
    },
    "required": [],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}