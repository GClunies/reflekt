{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Added Card/1-0.json",
    "description": "Server side event fired when a user adds a debit/credit card for payment.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Added Card",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "server",
        "owner": "@Surfline/squad-subscription",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "fingerprint": {
            "description": "The fingerprint of the card.",
            "type": "string"
        },
        "planId": {
            "description": "The ID of the subscription plan.",
            "type": "string"
        },
        "platform": {
            "description": "The platform the card was added on.",
            "enum": [
                "web",
                "ios",
                "android"
            ],
            "type": "string"
        }
    },
    "required": [
        "platform"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}