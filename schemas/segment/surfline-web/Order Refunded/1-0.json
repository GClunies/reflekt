{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Order Refunded/1-0.json",
    "description": "Fired when an order is refunded.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Order Refunded",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "server",
        "owner": "@Surfline/squad-subscription",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "category": {
            "description": "The category this event belongs to (e.g. `kbyg`, `ecommerce`, `sessions`, `travel`, `content`).",
            "enum": [
                "kbyg",
                "ecommerce",
                "sessions",
                "travel",
                "content"
            ],
            "type": "string"
        },
        "currency": {
            "description": "The currency.",
            "pattern": "[A-Z]",
            "type": "string"
        },
        "orderId": {
            "description": "The order ID.",
            "type": "string"
        },
        "total": {
            "description": "Total value ($) of the order.",
            "type": "number"
        }
    },
    "required": [
        "category",
        "currency",
        "orderId",
        "total"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}