{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Signed In/1-0.json",
    "description": "Web event fired when a user successfully signs in.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Signed In",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/squad-subscription",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "email": {
            "description": "The user's email.",
            "type": "string"
        },
        "location": {
            "description": "Where the event fired on our application.",
            "type": "string"
        },
        "method": {
            "description": "The method used to sign in.",
            "enum": [
                "email",
                "facebook",
                "forgot password flow"
            ],
            "type": "string"
        },
        "promotionId": {
            "description": "The promotion ID (if any).",
            "type": "string"
        },
        "userId": {
            "description": "The user's ID.",
            "type": "string"
        }
    },
    "required": [
        "method"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}