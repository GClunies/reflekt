{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Toggled Cam View/1-0.json",
    "description": "Web event fired when a user toggles a surf camera view.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Toggled Cam View",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/squad-realtime",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "camId": {
            "description": "The camera ID.",
            "type": "string"
        },
        "camName": {
            "description": "The camera name.",
            "type": "string"
        },
        "camNumber": {
            "description": "The camera number for the spot, where camNumber = 1 for the default camera for the spot.",
            "type": "integer"
        },
        "category": {
            "description": "The event category (used to group related events).",
            "type": "string"
        },
        "name": {
            "description": "The toggle type.",
            "type": "string"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subscriptionentitlement": {
            "description": "The user's entitlement.",
            "type": "string"
        },
        "view": {
            "description": "The view type.",
            "type": "string"
        }
    },
    "required": [
        "category",
        "view"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}