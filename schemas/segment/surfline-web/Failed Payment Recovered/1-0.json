{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Failed Payment Recovered/1-0.json",
    "description": "Server side event fired when a subscription payment that previously failed is processed successfully.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Failed Payment Recovered",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "server",
        "owner": "@Surfline/squad-subscription",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "currency": {
            "description": "The currency.",
            "pattern": "[A-Z]",
            "type": "string"
        },
        "entitlement": {
            "description": "The user's entitlement.",
            "type": "string"
        },
        "interval": {
            "description": "The subscription billing interval.",
            "enum": [
                "month",
                "year"
            ],
            "type": "string"
        },
        "planId": {
            "description": "The plan ID.",
            "type": "string"
        },
        "platform": {
            "description": "The platform the event is attributed to.",
            "enum": [
                "web",
                "ios",
                "android"
            ],
            "type": "string"
        },
        "revenue": {
            "description": "The transaction revenue.",
            "type": "number"
        },
        "subscriptionId": {
            "description": "The subscription ID.",
            "type": "string"
        }
    },
    "required": [
        "currency",
        "entitlement",
        "interval",
        "planId",
        "platform",
        "revenue",
        "subscriptionId"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}