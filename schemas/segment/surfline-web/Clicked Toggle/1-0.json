{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Clicked Toggle/1-0.json",
    "description": "Client side event fired when user clicks a toggle control",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Clicked Toggle",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/squad-web-platform",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "toggleEnd": {
            "description": "The value of the toggle after user interaction.",
            "type": "string"
        },
        "toggleStart": {
            "description": "The value of the toggle before user interaction.",
            "type": "string"
        },
        "toggleType": {
            "description": "Type of toggle. (e.g. highlights or surf alerts)",
            "type": "string"
        }
    },
    "required": [
        "toggleEnd",
        "toggleStart",
        "toggleType"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}