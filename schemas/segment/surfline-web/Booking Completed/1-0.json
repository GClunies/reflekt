{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Booking Completed/1-0.json",
    "description": "Fired when a user completes a travel booking.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Booking Completed",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "server",
        "owner": "@Surfline/squad-content",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "bookingId": {
            "description": "the id of the booking (received from Booking Layer webhook)",
            "type": "string"
        },
        "duration_in_days": {
            "description": "the number of days booked.",
            "type": "integer"
        },
        "duration_in_nights": {
            "description": "the number of nights booked.",
            "type": "integer"
        },
        "price": {
            "description": "The full price of the booking",
            "type": "string"
        },
        "travelSupplierId": {
            "description": "The ID of the supplier.",
            "type": "string"
        },
        "travelSupplierName": {
            "description": "The name of the supplier.",
            "type": "string"
        },
        "travelZone": {
            "description": "The zone where the Supplier is located (as designated by Bookinglayer).",
            "type": "string"
        }
    },
    "required": [
        "bookingId",
        "price",
        "travelSupplierId",
        "travelSupplierName",
        "travelZone"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}