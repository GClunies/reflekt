{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Selected Favorites/1-0.json",
    "description": "Web event fired when a user selects favorite spots during onboarding.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Selected Favorites",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/squad-subscription",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "numberFavorites": {
            "description": "The number of favorite spots selected.",
            "type": "integer"
        },
        "numberPopular": {
            "description": "The number of popular spots selected.",
            "type": "integer"
        },
        "numberRecommend": {
            "description": "The number of recommended spots selected.",
            "type": "integer"
        },
        "numberRegion": {
            "description": "The number of region spots selected.",
            "type": "integer"
        },
        "numberSearch": {
            "description": "The number of search spots selected.",
            "type": "integer"
        },
        "screenName": {
            "description": "The screen name.",
            "type": "string"
        },
        "spotsPopular": {
            "description": "The list of popular spots selected.",
            "type": "array"
        }
    },
    "required": [
        "numberFavorites",
        "numberPopular",
        "numberRecommend",
        "numberRegion",
        "numberSearch",
        "screenName",
        "spotsPopular"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}