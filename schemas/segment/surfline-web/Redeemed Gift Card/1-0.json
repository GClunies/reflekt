{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Redeemed Gift Card/1-0.json",
    "description": "Server side event fired when a user redeems a gift card.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Redeemed Gift Card",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "server",
        "owner": "@Surfline/squad-subscription",
        "priority": 2
    },
    "type": "object",
    "properties": {
        "billingPlatform": {
            "description": "Represents the payment processor for each subscription.",
            "enum": [
                "stripe",
                "ios",
                "android",
                "gift"
            ],
            "type": "string"
        },
        "planId": {
            "description": "The plan ID.",
            "type": "string"
        },
        "platform": {
            "description": "The platform the event is attributed to.",
            "enum": [
                "web",
                "ios",
                "android"
            ],
            "type": "string"
        },
        "redeemCode": {
            "description": "The redemption code for the gift card.",
            "type": "string"
        }
    },
    "required": [
        "planId",
        "platform",
        "redeemCode"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}