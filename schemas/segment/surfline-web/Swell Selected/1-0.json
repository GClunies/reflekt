{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Swell Selected/1-0.json",
    "description": "Fired when a user clicks a swell modal.",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Swell Selected",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/native-ios",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "forecastEnd": {
            "description": "When the forecast being viewed ends relative to today (4 = today + 3 days)",
            "type": "integer"
        },
        "forecastLength": {
            "description": "How many days are in forecast being viewed (4 = 4-day forecast is being viewed)",
            "type": "integer"
        },
        "forecastStart": {
            "description": "When the forecast being viewed starts relative to today (1 = today)",
            "type": "integer"
        },
        "spotId": {
            "description": "The spot ID.",
            "type": "string"
        },
        "spotName": {
            "description": "The spot name.",
            "type": "string"
        },
        "subregionId": {
            "description": "The subregion ID.",
            "type": "string"
        },
        "subregionName": {
            "description": "The subregion name.",
            "type": "string"
        },
        "swellNumber": {
            "description": "The swell the user clicks on.",
            "type": "number"
        },
        "tabName": {
            "description": "The tab name the user is on.",
            "enum": [
                "live",
                "forecast"
            ],
            "type": "string"
        }
    },
    "required": [
        "forecastStart",
        "forecastEnd",
        "forecastLength",
        "spotId",
        "spotName",
        "subregionId",
        "subregionName",
        "swellNumber",
        "tabName"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}