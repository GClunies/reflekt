{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "segment/surfline-web/Chart Player Updated/1-0.json",
    "description": "Fired when a user interacts with the chart player controls (play/pause/fast-forward/rewind/scrubber).",
    "self": {
        "vendor": "com.reflekt-ci",
        "name": "Chart Player Updated",
        "format": "jsonschema",
        "version": "1-0"
    },
    "metadata": {
        "fires_on": "client",
        "owner": "@Surfline/forecast-web",
        "priority": 1
    },
    "type": "object",
    "properties": {
        "action": {
            "description": "The action the user performed on the chart player.",
            "enum": [
                "play",
                "pause",
                "fast-forward",
                "rewind",
                "moved-scrubber"
            ],
            "type": "string"
        }
    },
    "required": [
        "action"
    ],
    "additionalProperties": false,
    "metrics": {
        "dimensions": [],
        "measures": []
    }
}